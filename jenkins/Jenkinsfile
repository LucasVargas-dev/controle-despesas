#!/usr/bin/env groovy
pipeline {
    agent none
    stages {
        stage('Build-back') {
            agent {
                docker { image 'nodejs' }
            }
            steps {
                sh '''cd back'''
            }
        }
        stage('Build-front') {
            agent {
                docker { image 'nodejs' }
            }
            steps {
                sh '''cd frontend'''
        }
    }
}